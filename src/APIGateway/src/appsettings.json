{
  "App": {
    "Name": "api-gateway"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "DiscoveryAddress": "http://localhost:8500",
    "Name": "api-gateway",
    "ID": "api-gateway",
    "Address": "localhost",
    "HealthCheckEndPoint": "api/healths"
  },
  "YarpServiceDiscovery": {
    "UpdateIntervalSeconds": 30
  },
  "ReserveYarp": {
    "routes": {
      "identity": {
        "clusterId": "identity",
        "match": {
          "path": "/api/identity/{**catch-all}"
        }
      }
    },
    "clusters": {
      "identity": {
        "destinations": {
          "identity": {
            "address": "identity-service"
          }
        }
      }
    }
  }
}
